# Stargate.ci Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
VITE_OPENAI_API_KEY=sk-your-openai-api-key-here

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY_TEST=pk_test_your-stripe-publishable-key-here
VITE_STRIPE_SECRET_KEY_TEST=sk_test_your-stripe-secret-key-here
VITE_STRIPE_PUBLISHABLE_KEY_PROD=pk_live_your-stripe-publishable-key-here
VITE_STRIPE_SECRET_KEY_PROD=sk_live_your-stripe-secret-key-here
VITE_STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret-here

# SendGrid Configuration
VITE_SENDGRID_API_KEY=SG.your-sendgrid-api-key-here
VITE_SENDGRID_FROM_EMAIL=noreply@stargate.ci
VITE_SENDGRID_FROM_NAME=Stargate.ci

# Mailchimp Configuration (Optional)
VITE_MAILCHIMP_API_KEY=your-mailchimp-api-key-here
VITE_MAILCHIMP_SERVER_PREFIX=us1
VITE_MAILCHIMP_LIST_ID=your-mailchimp-list-id-here

# Google Services Configuration
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# AWS Configuration (Optional)
VITE_AWS_ACCESS_KEY_ID=your-aws-access-key-id
VITE_AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=your-s3-bucket-name
VITE_AWS_CLOUDFRONT_DOMAIN=your-cloudfront-domain.cloudfront.net

# Firebase Configuration (Optional)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Auth0 Configuration (Optional)
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_CLIENT_SECRET=your-auth0-client-secret
VITE_AUTH0_AUDIENCE=your-auth0-audience

# Application Configuration
VITE_APP_NAME=Stargate.ci
VITE_APP_URL=https://stargate.ci
VITE_API_BASE_URL=http://localhost:8003/api/v1

# Development/Production Mode
VITE_NODE_ENV=development
